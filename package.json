{
  "name": "express-starter",
  "version": "0.1.0",
  "description": "Feature-based Express.js starter template with Prisma ORM",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "NODE_ENV=development nodemon src/server.ts",
    "prebuild": "rimraf dist",
    "build": "tsc --project tsconfig.json && tsx scripts/build.ts",
    "start": "node --import tsx/esm dist/index.js",
    "lint": "eslint --ext .ts src/",
    "lint:fix": "eslint --ext .ts src/ --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "prisma:generate": "prisma generate",
    "prisma:migrate:dev": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "seed": "tsx prisma/seeds/seed.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepare": "husky"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "express",
    "typescript",
    "prisma",
    "feature-based",
    "functional-programming"
  ],
  "author": "Okay",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^6.6.0",
    "bcrypt": "^5.1.1",
    "compression": "^1.8.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.0",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "hpp": "^0.2.3",
    "import-maps": "^0.2.4",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@eslint/js": "^9.24.0",
    "@faker-js/faker": "^9.7.0",
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/express-serve-static-core": "^5.0.6",
    "@types/hpp": "^0.2.6",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.14.1",
    "@typescript-eslint/eslint-plugin": "^8.30.1",
    "@typescript-eslint/parser": "^8.30.1",
    "@vitest/coverage-v8": "^3.1.1",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-import-resolver-typescript": "^4.3.2",
    "eslint-plugin-functional": "^9.0.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-unicorn": "^58.0.0",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.1",
    "nodemon": "^3.1.9",
    "prettier": "^3.5.3",
    "prisma": "^6.6.0",
    "rimraf": "^6.0.1",
    "tsc-files": "^1.1.4",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.3",
    "turbo": "^2.5.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.30.1",
    "vitest": "^3.1.1"
  },
  "imports": {
    "#api/*": "./dist/api/*.js",
    "#config/*": "./dist/config/*.js",
    "#features/*": "./dist/features/*.js",
    "#infrastructure/*": "./dist/infrastructure/*.js",
    "#shared/*": "./dist/shared/*.js",
    "#decorators/*": "./dist/shared/decorators/*.js",
    "#errors/*": "./dist/shared/errors/*.js",
    "#interceptors/*": "./dist/shared/interceptors/*.js",
    "#middleware/*": "./dist/shared/middleware/*.js",
    "#validators/*": "./dist/shared/validators/*.js",
    "#models/*": "./dist/shared/models/*.js",
    "#types/*": "./dist/types/*.js",
    "#utils/*": "./dist/utils/*.js",
    "#app/*": "./dist/app.ts/*.js",
    "#server/*": "./dist/server.ts/*.js",
    "#bootstrap/*": "./dist/bootstrap.ts/*.js"
  }
}
